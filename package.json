{
  "name": "create-src",
  "version": "0.0.3",
  "description": "A simple CLI tool to create a src directory with a advanced structure.",
  "scripts": {
    "test": "bun test",
    "prepare": "husky",
    "build": "npm run lint && tsc",
    "lab": "tsx --watch ./src/lab/index.ts",
    "start": "tsx --watch ./src/bin.ts",
    "tsc": "tsc --noEmit --watch",
    "lint": "tsc --noEmit && eslint .",
    "lint:prettier": "prettier . --write",
    "lint:fix": "eslint . --fix"
  },
  "bin": "./dist/bin.js",
  "license": "MIT",
  "keywords": [],
  "type": "commonjs",
  "homepage": "https://github.com/NazmusSayad/create-src#readme",
  "bugs": {
    "url": "https://github.com/NazmusSayad/create-src/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NazmusSayad/create-src.git"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.9",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.27.0",
    "@types/jszip": "^3.4.0",
    "@types/node": "^24.0.1",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-check-file": "^3.2.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.37.5",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1"
  },
  "dependencies": {
    "jszip": "^3.10.1",
    "noarg": "^3.1.9"
  }
}
