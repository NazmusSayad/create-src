{
  "name": "create-src",
  "version": "1.0.3",
  "description": "A simple CLI tool to create a project with a advanced structure.",
  "scripts": {
    "test": "bun test",
    "prepare": "husky",
    "dev": "tsc --watch",
    "build": "npm run lint && tsc",
    "start": "tsx --watch ./src/bin.ts",
    "tsc": "tsc --noEmit --watch",
    "lint": "tsc --noEmit && eslint .",
    "lint:format": "prettier . --write",
    "lint:fix": "eslint . --fix"
  },
  "bin": "./dist/bin.js",
  "license": "MIT",
  "keywords": [],
  "type": "commonjs",
  "homepage": "https://github.com/NazmusSayad/create-src#readme",
  "bugs": {
    "url": "https://github.com/NazmusSayad/create-src/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NazmusSayad/create-src.git"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.5.3",
    "cross-spawn": "^7.0.6",
    "jszip": "^3.10.1",
    "kleur": "^4.1.5",
    "noarg": "^3.1.9"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.9",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.27.0",
    "@types/cross-spawn": "^6.0.6",
    "@types/jszip": "^3.4.0",
    "@types/node": "^24.0.1",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-check-file": "^3.2.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.4.0",
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1"
  }
}
